void printIntMatrix(int[][] x, int r, int c) {
  int len;
  int i;
  int j;

  len = r * c;

  printssl("[| ");

  for (i = 0; i < r; i = i + 1) {
    for (j = 0; j < c; j = j + 1) {
      if (j != c - 1) {
        printsl($x);
        printssl(", ");
        x = x.+;
      } else {
        printsl($x);
        if (i != r - 1) {
          prints("");
        }
      }
    }
    if (i != r - 1) {
      printssl(" | ");
    }
  }

  prints(" |]");
}

int main() {
  int[3:3] a;
  int[3:3] b;

  a = [| 1, 2, 3 | 1, 2, 3 | 2, 2, 2 |];
  b = [| 1, 2, 3 | 1, 2, 3 | 2, 2, 2 |];

  printIntMatrix(@@a, a.rows, a.columns);

  return 0;
}
